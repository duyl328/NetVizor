{
  "AppSettings": {
    "AppName": "MyApp",
    "MaxConnection": 10
  },
  "Logging": {
    "EnableConsole": true,
    "EnableFile": true,
    "LogPath": "logs",
    "MaxFileSizeMB": 1,
    "RetentionDays": 30,
    "CompressDaysThreshold": 3,
    "MinimumLevel": "Information",
    "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
    "UseJsonFormat": false,
    "CompressCheckIntervalHours": 24
  }
}
